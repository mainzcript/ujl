									{
										"type": "card",
										"meta": {
											"id": "feature-brand-compliance-001",
											"updated_at": "2025-12-29T17:02:30.832Z",
											"_embedding": [1.6, 1.7, 1.8, 1.9, 2]
										},
										"fields": {
											"title": "Guaranteed Brand Compliance",
											"description": {
												"type": "doc",
												"content": [
													{
														"type": "paragraph",
														"content": [
															{
																"type": "text",
																"text": "Give your clients the freedom to edit content without fearing that they will break your design. When brand rules are built into the system, mistakes simply can't happen. Say goodbye to late-night support calls about accidentally changing the brand colour."
															}
														]
													}
												]
											}
										},
										"slots": {
											"content": []
										}
									},
									{
										"type": "card",
										"meta": {
											"id": "feature-accessibility-001",
											"updated_at": "2025-12-29T17:30:10.932Z",
											"_embedding": [2.1, 2.2, 2.3, 2.4, 2.5]
										},
										"fields": {
											"title": "Accessibility by Design",
											"description": {
												"type": "doc",
												"content": [
													{
														"type": "paragraph",
														"content": [
															{
																"type": "text",
																"text": "Accessibility isn't an afterthought — it's built in from the outset. Every page you create automatically meets WCAG standards. There are no expensive audits, painful retrofitting or legal headaches. You can be sure that your content is accessible every time."
															}
														]
													}
												]
											}
										},
										"slots": {
											"content": []
										}
									},
									{
										"type": "card",
										"meta": {
											"id": "feature-ai-safe-001",
											"updated_at": "2025-12-29T17:30:11.291Z",
											"_embedding": [2.6, 2.7, 2.8, 2.9, 3]
										},
										"fields": {
											"title": "AI-Safe",
											"description": {
												"type": "doc",
												"content": [
													{
														"type": "paragraph",
														"content": [
															{
																"type": "text",
																"text": "Use AI to create content without worrying about brand violations. Every layout that AI generates is automatically validated against your brand rules, enabling you to work quickly without compromising on what matters most."
															}
														]
													}
												]
											}
										},
										"slots": {
