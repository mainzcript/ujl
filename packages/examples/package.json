{
	"name": "@ujl-framework/examples",
	"version": "0.0.1",
	"description": "UJL Examples - Example Material and Test Data",
	"type": "module",
	"private": true,
	"files": [
		"src/"
	],
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./documents/showcase": "./src/documents/showcase.ujlc.json",
		"./themes/default": "./src/themes/default.ujlt.json"
	},
	"scripts": {
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"check": "echo 'No check script needed'",
		"test": "vitest",
		"clean": "echo 'No clean script needed'",
		"validate": "pnpm exec ujl-validate ./src/themes/default.ujlt.json && pnpm exec ujl-validate ./src/documents/showcase.ujlc.json"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@types/node": "^24.10.1",
		"@ujl-framework/types": "workspace:*",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^16.5.0",
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^3.2.4",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.4",
		"vitest": "3.2.3"
	}
}
