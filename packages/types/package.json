{
	"name": "@ujl-framework/types",
	"version": "0.0.1",
	"description": "UJL Types",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"exports": {
		".": "./dist/index.js"
	},
	"bin": {
		"ujl-validate": "./bin/ujl-validate.js"
	},
	"files": [
		"dist",
		"src",
		"bin"
	],
	"scripts": {
		"build": "pnpm run clean && tsc",
		"clean": "node -e \"require('fs').rmSync('dist', {recursive:true, force:true})\"",
		"check": "tsc --noEmit",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"validate": "node ./dist/validate.js",
		"prepare": "pnpm run build"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@types/node": "^24.10.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^16.5.0",
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^3.2.4",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.4"
	},
	"dependencies": {
		"zod": "^4.1.12"
	}
}
