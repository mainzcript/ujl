{
	"name": "@ujl-framework/adapter-web",
	"version": "0.0.1",
	"scripts": {
		"build": "vite build",
		"prepack": "vite build && publint",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*"
	],
	"sideEffects": [
		"**/*.css",
		"**/*.svelte"
	],
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.1",
		"@eslint/js": "^9.39.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/node": "^25.0.10",
		"@ujl-framework/adapter-svelte": "workspace:*",
		"@ujl-framework/types": "workspace:*",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.1.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"publint": "^0.3.17",
		"svelte": "^5.48.2",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1",
		"vite": "^7.3.1",
		"vite-plugin-dts": "^4.5.4"
	},
	"keywords": [
		"svelte"
	]
}
