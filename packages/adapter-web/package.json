{
	"name": "@ujl-framework/adapter-web",
	"version": "0.0.2",
	"scripts": {
		"build": "vite build",
		"prepack": "vite build && publint",
		"format": "prettier --write .",
		"check": "tsc --noEmit",
		"lint": "prettier --check . && eslint ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*"
	],
	"sideEffects": [
		"**/*.css",
		"**/*.svelte"
	],
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.2",
		"@eslint/js": "^10.0.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/node": "^25.3.1",
		"@ujl-framework/adapter-svelte": "workspace:*",
		"@ujl-framework/types": "workspace:*",
		"eslint": "^10.0.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.15.0",
		"globals": "^17.3.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
		"publint": "^0.3.17",
		"svelte": "^5.53.5",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.1",
		"vite": "^7.3.1",
		"vite-plugin-dts": "^4.5.4"
	},
	"keywords": [
		"svelte"
	],
	"author": "mainzcript GbR",
	"homepage": "https://ujl-framework.org",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mainzcript/ujl.git",
		"directory": "packages/adapter-web"
	},
	"license": "MIT"
}
