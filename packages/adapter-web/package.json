{
	"name": "@ujl-framework/adapter-web",
	"version": "0.0.1",
	"scripts": {
		"build": "vite build",
		"prepack": "vite build && publint",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css",
		"**/*.svelte"
	],
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^24.10.1",
		"@ujl-framework/adapter-svelte": "workspace:*",
		"@ujl-framework/types": "workspace:*",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.15",
		"svelte": "^5.43.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.4",
		"vite": "^7.2.2",
		"vite-plugin-dts": "^4.5.4"
	},
	"keywords": [
		"svelte"
	]
}
